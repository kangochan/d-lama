<div class="container-fluid">
  <div class="row user-search">
    <div class="col-md-10 col-md-offset-1">
      <div class="row">
        <% @users.each do |user| %>
          <div class="col-md-3 col-sm-4 col-xs-6">
            <%= link_to user_path(user) do %>
              <div class="image">
                <img src="" alt=""/>
              </div>
              <div class="user text-center">
                <p><%= user.nickname %></p>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
